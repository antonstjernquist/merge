{
  "name": "@merge/cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "agent-merge": "./bin/agent-merge.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "compile": "bun build ./src/index.ts --compile --outfile ./bin/agent-merge",
    "compile:linux-x64": "bun build ./src/index.ts --compile --target=bun-linux-x64 --outfile ./release/agent-merge-linux-x64",
    "compile:linux-arm64": "bun build ./src/index.ts --compile --target=bun-linux-arm64 --outfile ./release/agent-merge-linux-arm64",
    "compile:darwin-x64": "bun build ./src/index.ts --compile --target=bun-darwin-x64 --outfile ./release/agent-merge-darwin-x64",
    "compile:darwin-arm64": "bun build ./src/index.ts --compile --target=bun-darwin-arm64 --outfile ./release/agent-merge-darwin-arm64",
    "compile:windows-x64": "bun build ./src/index.ts --compile --target=bun-windows-x64 --outfile ./release/agent-merge-windows-x64.exe",
    "compile:all": "pnpm run compile:linux-x64 && pnpm run compile:linux-arm64 && pnpm run compile:darwin-x64 && pnpm run compile:darwin-arm64 && pnpm run compile:windows-x64"
  },
  "dependencies": {
    "@merge/shared-types": "workspace:*",
    "commander": "^12.0.0",
    "ws": "^8.16.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.3"
  }
}
